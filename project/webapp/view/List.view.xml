<mvc:View
    controllerName="project.controller.List"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        <List 
                            items="{/Orders}"
                        >
                            <items>
                                <StandardListItem
                                    title="{ShipName}"
                                />
                            </items>
                        </List>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
